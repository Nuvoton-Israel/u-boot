/dts-v1/;
#include "nuvoton-npcm750.dtsi"
#include "nuvoton-common-npcm7xx.dtsi"
#include "nuvoton-npcm730-mori-pincfg.dtsi"

/ {
	model = "Fii Mori Board (Device Tree v00.01)";
	compatible = "nuvoton,poleg", "mori";

	chosen {
		stdout-path = &serial0;
		tick-timer = &timer0;
	};

	aliases {
		serial0 = &serial0;
		serial1 = &serial1;
		serial2 = &serial2;
		serial3 = &serial3;
		i2c0 = &i2c0;
		i2c1 = &i2c1;
		i2c2 = &i2c2;
		i2c3 = &i2c3;
		i2c4 = &i2c4;
		i2c5 = &i2c5;
		i2c6 = &i2c6;
		i2c7 = &i2c7;
		i2c8 = &i2c8;
		i2c9 = &i2c9;
		i2c10 = &i2c10;
		i2c11 = &i2c11;
		i2c12 = &i2c12;
		i2c13 = &i2c13;
		i2c14 = &i2c14;
		i2c15 = &i2c15;
		spi0 = &fiu0;
		spi3 = &fiu3;
		spi4 = &fiux;
		spi5 = &spi0;
		mmc1 = &sdhci0;
		gpio0 = &gpio0;
		gpio1 = &gpio1;
		gpio2 = &gpio2;
		gpio3 = &gpio3;
		gpio4 = &gpio4;
		gpio5 = &gpio5;
		gpio6 = &gpio6;
		gpio7 = &gpio7;
		usb0 = &udc0;
		eth0 = &emc0;
		eth1 = &gmac0;
	};
};

&udc0 {
	status = "okay";
	phys = <&usbphy1 0>;
};

&fiu0 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi0cs1_pins>;
	status = "okay";
	spi_flash@0 {
		compatible = "spi-flash";
		reg = <0>; /* Chip select 0 */
		memory-map = <0x80000000 0x4000000>;
	};
	spi_flash@1 {
		compatible = "spi-flash";
		reg = <1>;
		memory-map = <0x88000000 0x4000000>;
	};
};

&fiu3 {
	pinctrl-names = "default";
	pinctrl-0 = <&spi3_pins
			&spi3quad_pins>;
	status = "okay";
	spi-nor@0 {
		compatible = "spi-flash";
		reg = <0>; /* Chip select 0 */
		memory-map = <0xA0000000 0x8000000>;
	};
};

&fiux {
	status = "okay";
	spi_flash@0 {
		compatible = "spi-flash";
		reg = <0>; /* Chip select 0 */
	};
};

&aes {
	status = "okay";
};

